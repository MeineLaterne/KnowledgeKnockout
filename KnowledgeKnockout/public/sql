-- Exportiere Struktur von Tabelle kk_db.avatar
DROP TABLE IF EXISTS `avatar`;
CREATE TABLE IF NOT EXISTS `avatar` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userId` int(11) NOT NULL,
  `level` int(11) NOT NULL DEFAULT '1',
  `topicBlockId` int(11) NOT NULL,
  `imageSrc` varchar(512) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `userId` (`userId`),
  KEY `FK_avatar_topicblock` (`topicBlockId`),
  CONSTRAINT `FK_avatar_topicblock` FOREIGN KEY (`topicBlockId`) REFERENCES `topicblock` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
